{
  "name": "nest-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "postinstall": "npx husky install",
    "format:eslint": "eslint src --ext .ts --fix",
    "format:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "format": "yarn run format:eslint && yarn run format:prettier",
    "lint:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "lint:type": "tsc",
    "lint": "yarn run lint:lint && yarn run lint:type",
    "test": "vitest --segfault-retry=5",
    "test:coverage": "npm run test -- --coverage",
    "test:ui": "npm run test -- --ui",
    "test:e2e": "npx playwright test",
    "prepare": "husky install"
  },
  "dependencies": {
    "@headlessui/react": "1.7.17",
    "@heroicons/react": "2.1.1",
    "eslint-plugin-jest": "27.9.0",
    "next": "14.0.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "5.3.3"
  },
  "devDependencies": {
    "@axe-core/playwright": "4.8.5",
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "18.4.4",
    "@playwright/test": "1.42.1",
    "@testing-library/jest-dom": "6.1.4",
    "@testing-library/react": "14.2.1",
    "@testing-library/user-event": "14.5.2",
    "@types/node": "20.10.6",
    "@types/react": "18.2.74",
    "@types/react-dom": "18.2.23",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "@vitejs/plugin-react": "4.2.1",
    "@vitest/coverage-v8": "0.34.6",
    "@vitest/ui": "0.34.7",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-standard-with-typescript": "37.0.0",
    "eslint-plugin-import": "2.29.0",
    "eslint-plugin-n": "16.6.2",
    "eslint-plugin-playwright": "1.5.2",
    "eslint-plugin-prettier": "5.1.3",
    "eslint-plugin-promise": "6.1.1",
    "eslint-plugin-simple-import-sort": "12.0.0",
    "eslint-plugin-sonarjs": "0.24.0",
    "eslint-plugin-unicorn": "51.0.1",
    "happy-dom": "12.10.3",
    "husky": "8.0.3",
    "postcss": "8.4.35",
    "prettier": "3.2.5",
    "prettier-plugin-tailwindcss": "0.5.11",
    "tailwindcss": "3.4.1",
    "vite-tsconfig-paths": "4.3.2",
    "vitest": "0.34.6"
  }
}
