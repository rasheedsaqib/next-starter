{
  "name": "next-starter",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "analyze:lint": "eslint --cache --cache-location ./node_modules/.cache/eslint .",
    "analyze:type": "tsc",
    "analyze": "npm run analyze:lint && npm run analyze:type",
    "lint:eslint": "eslint app --ext .ts --fix",
    "lint:prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,md}\"",
    "lint": "npm run lint:eslint && npm run lint:prettier"
  },
  "dependencies": {
    "next": "13.1.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "devDependencies": {
    "@types/node": "18.11.18",
    "@types/react": "18.0.26",
    "@types/react-dom": "18.0.10",
    "@typescript-eslint/eslint-plugin": "^5.48.1",
    "eslint": "8.31.0",
    "eslint-config-next": "13.1.2",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard-with-typescript": "^27.0.1",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-sonarjs": "^0.18.0",
    "eslint-plugin-unicorn": "^45.0.2",
    "prettier": "^2.8.2",
    "prettier-plugin-tailwindcss": "^0.2.1"
  }
}
